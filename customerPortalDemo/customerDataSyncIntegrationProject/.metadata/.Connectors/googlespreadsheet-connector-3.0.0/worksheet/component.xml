<component name="worksheet" type="synapse/template">
    <subComponents>
        <component name="getSheetMetaData">
            <file>getSheetMetaData.xml</file>
            <description>provides a feed to access the sheet metadata contained within a given
                spreadsheet.
            </description>
        </component>
        <component name="addSheetBatchRequest">
            <file>addSheetBatchRequest.xml</file>
            <description>Add a sheet or multiple sheets to a spreadsheet.</description>
        </component>
        <component name="updateSheetPropertiesBatchRequest">
            <file>updateSheetPropertiesBatchRequest.xml</file>
            <description>Update all sheet properties.</description>
        </component>
        <component name="deleteSheetBatchRequest">
            <file>deleteSheetBatchRequest.xml</file>
            <description>Remove a sheet or multiple sheets from a given spreadsheet.</description>
        </component>
        <component name="copyTo">
            <file>copyTo.xml</file>
            <description>Copies a single sheet from a spreadsheet to another spreadsheet.
            </description>
        </component>
    </subComponents>
</component>